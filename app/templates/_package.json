{
  "name": "<%= _.slugify(name) %>",
  "version": "1.0.0",
  "description": "<%= description %>",
  "keywords": [ "<%= _.slugify(name) %>", "pandora", "widget", "seajs" ],
  "homepage": "https://github.com/pandorajs/<%= _.slugify(name) %>",
  "bugs": {
    "url": "https://github.com/pandorajs/<%= _.slugify(name) %>/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/pandorajs/<%= _.slugify(name) %>/blob/master/LICENSE-MIT"
    }
  ],
  "author": {
    "name": "pandorajs",
    "url": "https://github.com/pandorajs"
  },
  "main": "Gruntfile.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/pandorajs/<%= _.slugify(name) %>.git"
  },
  "scripts": {
    "test": "grunt qunit"
  },
  "engines": {
    "node": ">= 0.8.0"
  },
  "dependencies": {
  },
  "devDependencies": {
    "grunt": "*",
    "grunt-cmd-transport": "crossjs/grunt-cmd-transport",
    "grunt-cmd-concat": "*",
    "grunt-contrib-clean": "*",
    "grunt-contrib-copy": "*",
    "grunt-contrib-jshint": "*",
    "grunt-contrib-uglify": "*",
    "grunt-contrib-yuidoc": "*",
    "grunt-coveralls": "*",
    "grunt-qunit-istanbul": "*",
    "matchdep": "*"
  },
  "family": "pandora",
  "spm": {
    "output": [ "<%= _.slugify(name) %>.js" ],
    "alias": {
      "<%= _.slugify(extendedby) %>": "pandora/<%= _.slugify(extendedby) %>/1.0.0/<%= _.slugify(extendedby) %>"
    }
  }
}
